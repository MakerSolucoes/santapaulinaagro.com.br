{
  "name": "makersolucoes.com.br/makersoluces",
  "description": "makersolucoes",
  "require": {
    "roots/wordpress": "^6.1",
    "wpackagist-plugin/advanced-custom-fields-font-awesome": "4.0.5",
    "wpackagist-plugin/contact-form-7": "5.7.6",
    "wpackagist-plugin/custom-post-type-ui": "1.13.5",
    "wpackagist-plugin/database-for-cf7": "1.2.3",
    "wpackagist-plugin/w3-total-cache": "2.3.2"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "scripts": {
    "move-wordpress": [
      "rm -rf wordpress/wp-content/plugins/*",
      "rm -rf wordpress/wp-content/themes/*",
      "cp -rf wordpress/* .",
      "rm -rf wordpress"
    ],
    "post-install-cmd": [
      "@move-wordpress"
    ],
    "post-update-cmd": [
      "@move-wordpress"
    ]
  },
  "config": {
    "allow-plugins": {
      "roots/wordpress-core-installer": true,
      "composer/installers": true
    }
  }
}
